{
    "extends": ["config:base"],
    "regexManagers": [
      {
        "fileMatch": [
            "security-actions/semgrep/action.yml"
        ],
        "matchStrings": [
          "semgrep==(?<currentValue>\\d+\\.\\d+\\.\\d+)"
        ],
        "depNameTemplate": "semgrep",
        "datasourceTemplate": "pypi"
      }
    ]
  }
  